{
	// Enables the 'What's This' Tips when you see a new thing.
	"whatsThis": true,
	// Makes the text shadow render 1/3 of a pixel down and to the right rather than a full pixel
	"closerTextShadows": true,
	// Makes the tooltip rendering similar to LCE.
	"tooltips": true,
	// Makes the chat's width the whole screen
	"chatWidth": true,
	// Makes the chat's Y position the same as LCE (almost; it's actually 1 pixel off)
	"chatYPos": true,
	// Recolors the chat to LCE's color
	"recolorChat": true,
	// Makes the chat's text 2/3 of its normal size
	"rescaleChatText": false,
	// Hides the In Game HUD when a diffrent UI is displayed
	"hideHudWhenInUI": true,
	// Hides the Dark Overlay that appears in UI's
	"removeTransparentBG": true,
	// Renders the panorama in place of the normal options background
	"renderPanoramaEverywhere": false,
	// Makes the panorama a 2D scrolling texture, like LCE.
	"lcePan": true,
	// Prioritises L4J Panorama textures over LCEUI's
	"l4jPanSupport": true
}